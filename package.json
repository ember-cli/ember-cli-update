{
  "name": "ember-cli-update",
  "version": "0.2.0",
  "description": "Update Ember CLI Ember.js apps and addons",
  "main": "src/index.js",
  "bin": {
    "ember-cli-update": "bin/ember-cli-update.js"
  },
  "scripts": {
    "test": "npm run test:bin && npm run test:command",
    "test:bin": "cross-env DEBUG=ember-cli-update,git-diff-apply mocha \"test/!(fixtures)/**/!(ember-addon)-test.js\"",
    "test:command": "cross-env DEBUG=ember-cli-addon-tests mocha test/acceptance/ember-addon-test.js",
    "posttest": "npm run lint",
    "prelint": "eslint --version",
    "lint": "eslint bin src test",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kellyselden/ember-cli-update.git"
  },
  "keywords": [
    "ember.js",
    "ember-addon",
    "ember-cli"
  ],
  "author": "Kelly Selden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyselden/ember-cli-update/issues"
  },
  "homepage": "https://github.com/kellyselden/ember-cli-update#readme",
  "dependencies": {
    "debug": "^2.6.8",
    "git-diff-apply": "^0.1.4",
    "semver": "^5.4.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "cross-env": "^5.0.1",
    "ember-cli": "^2.14.2",
    "ember-cli-addon-tests": "^0.9.0",
    "eslint": "^4.3.0",
    "eslint-config-sane": "^0.1.0",
    "fixturify": "^0.3.3",
    "fs-extra": "^4.0.0",
    "mocha": "^3.4.2"
  },
  "ember-addon": {
    "main": "src/ember-addon.js"
  }
}
