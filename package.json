{
  "name": "ember-cli-update",
  "version": "0.48.1",
  "description": "Update Ember CLI Ember.js apps and addons (and Glimmer.js apps)",
  "main": "src/index.js",
  "bin": {
    "ember-cli-update": "bin/ember-cli-update.js",
    "ecu": "bin/ember-cli-update.js"
  },
  "scripts": {
    "start": "bin/ember-cli-update.js",
    "test": "cross-env DEBUG=ember-cli-update,boilerplate-update,git-diff-apply,git-fixtures mocha \"test/!(fixtures)/**/*-test.js\"",
    "test:command": "cross-env DEBUG=ember-cli-update,boilerplate-update,git-diff-apply,git-fixtures mocha test/acceptance/ember-addon-test.js --retries 1",
    "test:bin": "cross-env DEBUG=ember-cli-update,boilerplate-update,git-diff-apply,git-fixtures mocha test/acceptance/ember-cli-update-test.js --retries 1",
    "test:fast": "cross-env DEBUG=ember-cli-update,boilerplate-update,git-diff-apply,git-fixtures mocha \"test/+(integration|unit)/**/*-test.js\" --retries 1",
    "lint": "eslint . --ext js,json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ember-cli/ember-cli-update.git"
  },
  "keywords": [
    "ember.js",
    "ember-addon",
    "ember-cli"
  ],
  "author": "Kelly Selden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-update/issues"
  },
  "homepage": "https://github.com/ember-cli/ember-cli-update#readme",
  "engines": {
    "node": ">=8.10"
  },
  "files": [
    "bin",
    "src"
  ],
  "dependencies": {
    "boilerplate-update": "0.31.5",
    "debug": "^4.1.1",
    "execa": "^3.4.0",
    "fs-extra": "^8.0.0",
    "inquirer": "^7.0.0",
    "npm-package-arg": "^7.0.0",
    "resolve": "^1.10.0",
    "semver": "^6.0.0",
    "tmp": "0.1.0",
    "update-notifier": "^4.0.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "chai-string": "^1.5.0",
    "cross-env": "^6.0.0",
    "ember-cli": "3.11.0-beta.1",
    "eslint": "^6.1.0",
    "eslint-config-sane": "0.8.5",
    "eslint-config-sane-node": "0.2.2",
    "eslint-plugin-json-files": "0.8.0",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "git-diff-apply": "0.20.3",
    "git-fixtures": "^2.1.0",
    "mocha": "^6.0.0",
    "mocha-helpers": "^4.0.0",
    "renovate-config-standard": "^2.0.0",
    "sinon": "^8.0.0",
    "sinon-chai": "^3.3.0"
  },
  "ember-addon": {
    "main": "src/ember-addon.js"
  }
}
